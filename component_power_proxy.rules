CombinedBatteryStorageSmall
{
	Type = MultiResourceStorage
	ResourceType = battery
	//ResourceStorages = [BatteryStorage]
	ResourceStorages = [ BatteryStorage ]
	SuppliesResources = true
	MinResourcesPickUp = 1000
	MaxResourcesPickUp = 1000
	MaxResources = 8000
	PickUpRate = 1000 / 0.5
	ViaBuffs
	{
		OutgoingBuffProviders = [BuffProvider, BuffProviderCapacitor]
		ComponentID = BatteryStorage
	}
	AddMode = PrioritizeLeastResources
	ReceiveResourceMediaEffects = &/COMMON_EFFECTS/PowerDeliver
}

